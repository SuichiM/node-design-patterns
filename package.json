{
  "name": "design_patterns_repo",
  "type": "module",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'node_modules' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/server.ts",
    "test": "mocha dist/**/*.spec.js",
    "lint": "eslint src --ext js",
    "start": "ts-node dist/server.js",
    "buildstart": "tsc -p src/server.ts --out dist/main.js && ts-node dist/main.js",
    "build": "tsc src/server.ts"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "docker",
    "express"
  ],
  "author": "SuichiM",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^7.0.22",
    "express": "^4.15.3",
    "pug": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/mocha": "2.2.41",
    "es6-interface": "^3.0.9",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.19.0",
    "eslint-plugin-promise": "3.5.0",
    "mocha": "3.4.2",
    "nodemon": "^1.19.0",
    "supertest": "3.0.0"
  }
}
